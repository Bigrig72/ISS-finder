</head>

<body>
  <div id="map"></div>
  <form id="searchForm">
    <input id="inputLoc" type="text" name="search_field" placeholder="Enter location" required>
    <button id="locButton" type="submit">Search!</button>
  </form>
  <div id="results">
    <p id="location"></p>
    <p id="distance"></p>
  </div>
</body>

